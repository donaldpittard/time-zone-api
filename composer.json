{
    "name": "donaldpittard/time-zone-api",
    "description": "A time zone suggestion API",
    "keywords": ["time zone", "rest"],
    "homepage": "https://github.com/donaldpittard/time-zone-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Donald Pittard",
            "email": "donaldpittard@gmail.com",
            "homepage": "http://www.donaldpittard.com/"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "components/jquery": "^3.3",
        "components/jqueryui": "^1.12"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0",
        "heroku/heroku-buildpack-php": "^142.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public public/index.php",
        "test": "phpunit"
    }

}
